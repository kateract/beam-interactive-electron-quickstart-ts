    "@angular/compiler": "^4.2.6",
    "@angular/core": "^4.2.6",
    "@angular/forms": "~4.2.6",
    "@angular/http": "^4.2.6",
    "@angular/platform-browser": "^4.2.6",
    "@angular/platform-browser-dynamic": "^4.2.6",
    "@angular/router": "^4.2.6",
    "@angular/upgrade": "^4.2.6",
    "angular-in-memory-web-api": "^0.3.2",
    "core-js": "^2.4.1",
    "jasmine": "^2.6.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.4.2",
    "systemjs": "^0.20.15",
    "zone.js": "^0.8.13"
